#include<iostream>
using namespace std;
int find_largest(int nums[],int n){
    return *max_element(nums,nums+n);
}
int main()
{
    int nums[]={5,6,7,8,9};
    int n = size of(nums)/sizeof(nums[0]);
    cout <<"original array:";
    for (int i=0;i<n;i++)
    cout<< nums[i]<<"";
    cout<< "\nlargest element of the said array:"<< find_largest(nums,n);
    return 0;
}
